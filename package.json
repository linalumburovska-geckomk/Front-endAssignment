{
  "name": "front-endassignment",
  "version": "1.0.0",
  "description": "FRONT-END EXERCISE ASSIGNMENT",
  "private": true,
  "main": "./src/app.js",
  "dependencies": {
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mocha": "^6.0.2",
    "requirejs": "^2.3.6",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "start": "run-s build watch:run",
    "watch:run": "run-p watch:js watch:css server",
    "server": "node server",
    "build": "run-s build:js build:css",
    "watch:css": "watch -p './css/**/*.scss' -c 'npm run build:css'",
    "watch:js": "watch -p './src/**/*.js' -c 'npm run build:js'",
    "build:css": "node-sass --output-style compressed css/style.scss public/style.css",
    "build:js": "browserify src/app.js -o public/script.js -t [ babelify --presets [ @babel/preset-env ] --plugins [ @babel/plugin-proposal-class-properties ] ]",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linalumburovska-geckomk/Front-endAssignment.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/linalumburovska-geckomk/Front-endAssignment/issues"
  },
  "homepage": "https://github.com/linalumburovska-geckomk/Front-endAssignment#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "watch-cli": "^0.2.3"
  }
}
